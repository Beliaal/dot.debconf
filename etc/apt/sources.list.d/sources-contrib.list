# debian stable main repository, apt-cache safe...
deb http://deb.debian.org/debian/ stable contrib
deb-src http://deb.debian.org/debian/ stable contrib

# stable-updates, previously known as 'volatile'
deb http://deb.debian.org/debian/ stable-updates contrib
deb-src http://deb.debian.org/debian/ stable-updates contrib

# debian-security stable...
deb http://security.debian.org/debian-security stable-security contrib
deb-src http://security.debian.org/debian-security stable-security contrib
